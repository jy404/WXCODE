/* pages/modal/index.wxss */
/**内容部分**/
.topview{
  width: 100%;
  height: 50rpx;
  border-bottom: 1rpx solid #ebebeb;
}
/**标题部分**/
.titleview{
  width:48%;
  height: 50rpx;
  padding-left: 40%;
  line-height:40rpx;
  font-size: 30rpx;
  font-weight: 600rpx;
  float: left;
}
/**关闭按钮**/
.closeview{
  float: right;
  width: 40rpx;
  height:40rpx; 
  margin:5rpx 20rpx; 
}
.closecss{
  width: 100%;
  height: 100%;  

}
/**产品图片**/
.proinfoimg{
  width: 100%;
  height: 230rpx;
  text-align: center;
}
.proinfoimgcss{
  width: 230rpx;
  height: 230rpx;
}

